{
    "name": "indigophp/supervisor",
    "description": "PHP library for Supervisor",
    "license": "MIT",
    "keywords": ["supervisor", "process manager"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/options-resolver": "~2.6"
    },
    "require-dev": {
        "lstrojny/fxmlrpc": "dev-master",
        "indigophp/http-adapter": "dev-develop",
        "guzzlehttp/guzzle": "~5.0.0",
        "zendframework/zend-xmlrpc": "~2.3.0,>=2.3.3",
        "phpspec/phpspec": "2.1.*@dev,>=2.1.0-RC2",
        "henrikbjorn/phpspec-code-coverage" : "1.0.*@dev",
        "behat/behat": "~3.0@dev",
        "rhumsaa/array_column": "~1.1.0"
    },
    "suggest": {
        "lstrojny/fxmlrpc": "A modern, super fast XML/RPC client for PHP >=5.4",
        "guzzlehttp/streams": "GuzzleHttp Streams for Guzzle Event Processor",
        "zendframework/zend-xmlrpc": "XmlRpc component from Zend Framework 2"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Supervisor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indigo\\Supervisor\\Stub\\": "stub/"
        },
        "files": ["src/functions.php"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
