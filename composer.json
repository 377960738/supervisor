{
    "name": "supervisorphp/supervisor",
    "description": "PHP library for managing Supervisor through XML-RPC API",
    "license": "MIT",
    "keywords": ["supervisor", "process manager"],
    "homepage": "http://supervisorphp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "fxmlrpc/fxmlrpc": "dev-initial_import"
    },
    "require-dev": {
        "fxmlrpc/client": "dev-initial_import",
        "fxmlrpc/native-serialization": "dev-initial_import",
        "php-http/guzzle5-adapter": "dev-initial_import",
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0",
        "behat/behat": "~3.0",
        "ramsey/array_column": "~1.1",
        "supervisorphp/configuration": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Supervisor\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
