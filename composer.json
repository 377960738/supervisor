{
    "name": "indigophp/supervisor",
    "description": "PHP library for Supervisord",
    "license": "MIT",
    "keywords": ["supervisor", "process manager"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-xmlrpc": "*",
        "symfony/options-resolver": "~2.4.0",
        "symfony/process": "~2.4.0",
        "psr/log": "~1.0",
        "kriswallsmith/buzz": "v0.10",
        "jakeasmith/http_build_url": "~0.1.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev",
        "symfony/console": "~2.4.0"
    },
    "suggest": {
        "symfony/console": "Used for CLI application"
    },
    "autoload": {
        "psr-0": {
            "Buzz\\": "lib/"
        },
        "psr-4": {
            "Indigo\\Supervisor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indigo\\Supervisor\\Test\\": "tests/"
        }
    }
}
