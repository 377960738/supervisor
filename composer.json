{
    "name": "indigophp/supervisor",
    "description": "PHP library for Supervisord",
    "license": "MIT",
    "keywords": ["supervisor", "process manager"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/options-resolver": "~2.4",
        "symfony/process": "~2.4",
        "league/event": "~1.0.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "lstrojny/fxmlrpc": "dev-master",
        "guzzlehttp/streams": "~2.1.0",
        "zendframework/zend-xmlrpc": "~2.3.0",
        "zendframework/zendxml": "~1.0.0",
        "phpspec/phpspec": "2.1.*@dev,>=2.1.0-RC2",
        "henrikbjorn/phpspec-code-coverage" : "1.0.*@dev"
    },
    "suggest": {
        "lstrojny/fxmlrpc": "A modern, super fast XML/RPC client for PHP >=5.4",
        "guzzlehttp/streams": "GuzzleHttp 4 Streams for Guzzle Event Processor",
        "zendframework/zend-xmlrpc": "XmlRpc component from Zend Framework 2",
        "zendframework/zendxml": "Required by Zend XmlRpc component"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Supervisor\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
