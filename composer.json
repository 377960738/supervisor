{
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        },
        {
            "name": "Buster Neece",
            "email": "buster@busterneece.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Supervisor\\": "src/"
        }
    },
    "description": "PHP library for managing Supervisor through XML-RPC API",
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "homepage": "http://supervisorphp.com",
    "keywords": [
        "supervisor",
        "process manager"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "supervisorphp/supervisor",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "behat/behat": "^3.0",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "guzzlehttp/guzzle": "^6.0",
        "laminas/laminas-xmlrpc": "^2.9",
        "lstrojny/fxmlrpc": "^0.15",
        "phpspec/phpspec": "^6.2",
        "supervisorphp/configuration": "^0.2",
        "php-http/guzzle6-adapter": "^2.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "php-http/httplug": "^2.1",
        "php-http/message": "^1.8"
    },
    "scripts": {
        "ci": [
            "@composer update --prefer-dist --no-progress --no-suggest",
            "@test-ci",
            "@behat"
        ],
        "test": "vendor/bin/phpspec run",
        "test-ci": "vendor/bin/phpspec run -c phpspec.yml.ci",
        "behat": "vendor/bin/behat"
    },
    "suggest": {
        "lstrojny/fxmlrpc": "A modern, super fast XML/RPC client for PHP >=5.4",
        "laminas/laminas-xmlrpc": "XmlRpc Laminas component"
    }
}
